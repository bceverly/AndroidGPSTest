<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/title_text" android:text="@string/title_text" android:gravity="center_horizontal"></TextView>
    <TableLayout android:layout_width="fill_parent" android:id="@+id/tableLayout1" android:layout_height="wrap_content">
        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:text="@string/latitude" android:id="@+id/textView1" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
            <TextView android:text="TextView" android:id="@+id/longitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
        </TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow2" android:layout_height="wrap_content">
    		<TextView android:text="@string/longitude" android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    		<TextView android:text="TextView" android:id="@+id/latitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow2" android:layout_height="wrap_content">
    		<TextView android:text="@string/altitude" android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    		<TextView android:text="TextView" android:id="@+id/altitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    </TableLayout>
</LinearLayout>
