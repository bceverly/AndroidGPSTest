<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
    <TextView android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/title_text" android:text="@string/title_text" android:gravity="center_horizontal" android:layout_marginTop="40dip"></TextView>
    <TableLayout android:layout_width="fill_parent" android:id="@+id/tableLayout1" android:layout_height="wrap_content">
        <TableRow android:id="@+id/tableRow1" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:layout_width="wrap_content" android:id="@+id/textView1" android:text="@string/latitude" android:layout_height="wrap_content" android:gravity="right"></TextView>
            <TextView android:text="" android:id="@+id/longitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
        </TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow2" android:layout_height="wrap_content">
    		<TextView android:layout_width="wrap_content" android:id="@+id/textView3" android:text="@string/longitude" android:layout_height="wrap_content" android:gravity="right"></TextView>
    		<TextView android:text="" android:id="@+id/latitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow2" android:layout_height="wrap_content">
    		<TextView android:text="@string/altitude" android:id="@+id/textView3" android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="right"></TextView>
    		<TextView android:text="" android:id="@+id/altitude" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow3" android:layout_height="wrap_content">
    		<TextView android:text="@string/accuracy" android:id="@+id/textView4" android:layout_width="wrap_content" android:layout_height="wrap_content" android:gravity="right"></TextView>
    		<TextView android:text="" android:id="@+id/accuracy" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow4" android:layout_height="wrap_content">
    		<TextView android:text="@string/accuracy_description" android:id="@+id/textView5" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="50dip"></TextView>
    	</TableRow>
    	<TableRow android:layout_width="fill_parent" android:id="@+id/tableRow5" android:layout_height="wrap_content">
    		<TextView android:text="@string/accuracy_description2" android:id="@+id/textView6" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    	</TableRow>
    </TableLayout>
</LinearLayout>
